{
    "name": "Compliance Sentinel MCP Server",
    "description": "A Model Context Protocol server for real-time security analysis and compliance checking",
    "repository": "https://github.com/kalisnetwork/compliance-sentinel",
    "logo": "https://raw.githubusercontent.com/kalisnetwork/compliance-sentinel/main/docs/logo.png",
    "keywords": [
        "security",
        "compliance",
        "mcp",
        "analysis",
        "python"
    ],
    "image": "heroku/python",
    "stack": "heroku-22",
    "buildpacks": [
        {
            "url": "heroku/python"
        }
    ],
    "formation": {
        "web": {
            "quantity": 1,
            "size": "eco"
        }
    },
    "env": {
        "PYTHONPATH": {
            "description": "Python path for imports",
            "value": "."
        },
        "LOG_LEVEL": {
            "description": "Logging level",
            "value": "INFO"
        },
        "WEB_CONCURRENCY": {
            "description": "Number of web workers",
            "value": "1"
        }
    },
    "scripts": {
        "postdeploy": "echo 'Compliance Sentinel MCP Server deployed successfully!'"
    },
    "success_url": "/",
    "website": "https://github.com/kalisnetwork/compliance-sentinel"
}