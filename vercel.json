{
    "version": 2,
    "name": "compliance-sentinel-mcp",
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/api/index"
        }
    ],
    "env": {
        "PYTHONPATH": ".",
        "LOG_LEVEL": "INFO"
    },
    "functions": {
        "api/index.py": {
            "maxDuration": 30
        }
    }
}